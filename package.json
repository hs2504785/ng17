{
  "name": "ng17",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run uncss",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ng17": "node dist/ng17/server/server.mjs",
    "sme-build": "ng build --source-map",
    "sme": "npm run sme-build && source-map-explorer dist/**/*.js --no-border-checks",
    "uncss": "node tools/scripts/uncss.js",
    "purgecss": "purgecss --config tools/scripts/purgecss.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.3",
    "@angular/common": "^17.3.3",
    "@angular/compiler": "^17.3.3",
    "@angular/core": "^17.3.3",
    "@angular/forms": "^17.3.3",
    "@angular/platform-browser": "^17.3.3",
    "@angular/platform-browser-dynamic": "^17.3.3",
    "@angular/platform-server": "^17.3.3",
    "@angular/router": "^17.3.3",
    "@angular/ssr": "^17.3.3",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "purgecss": "^5.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
}